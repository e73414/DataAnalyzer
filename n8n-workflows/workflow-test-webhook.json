{
  "name": "Test Webhook Debug",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "test-debug",
        "responseMode": "responseNode",
        "options": {}
      },
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2,
      "position": [200, 300],
      "id": "test-webhook",
      "name": "Test Webhook"
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ { received: $json, body: $json.body, email_direct: $json.email, email_body: $json.body?.email } }}",
        "options": {}
      },
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1.1,
      "position": [400, 300],
      "id": "test-respond",
      "name": "Test Response"
    }
  ],
  "connections": {
    "Test Webhook": {
      "main": [[{ "node": "Test Response", "type": "main", "index": 0 }]]
    }
  },
  "settings": {
    "executionOrder": "v1"
  }
}
